<Window x:Class="pfVisualisator.NewTagoElement"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Выбор тегов" Height="400" Width="659">
    <StackPanel HorizontalAlignment="Center" Name="spNewTago" VerticalAlignment="Top">
        <GroupBox Header="Пометьте нужные теги" Name="groupBox1" >
            <Grid>
                <ListBox  Name="LBVan" VerticalAlignment="Top">
                </ListBox>
            </Grid>
        </GroupBox>
        <StackPanel Orientation="Horizontal" Margin="3">
            <Label Name="pdescrLb">Новый тег:</Label>
            <TextBox Name="addostrokeTBx" Width="260" HorizontalAlignment="Right" Text=""></TextBox>
            <Label Name="pSdvigLb1"></Label>
            <Button Name="AddTagoBtn" Click="AddTago_Click">Add</Button>
        </StackPanel>
        <StackPanel Orientation="Horizontal" Margin="3" HorizontalAlignment="Center">
            <Button Name="savoBtn" Margin="13" Click="savoBtn_Click">Save</Button>
            <Button Name="canceloBtn" Margin="13" Click="canceloBtn_Click">Cancel</Button>
        </StackPanel>

    </StackPanel>
    
</Window>
